{
  "builders": [{
    "type": "qemu",
    "iso_url": "{{user `images_path`}}/vEOS-lab-4.20.1F-combined.vmdk",
    "iso_checksum": "{{user `iso_checksum`}}",
    "iso_checksum_type": "{{user `iso_checksum_type`}}",
    "output_directory": "output-arista-veos-4.20.1F-{{build_type}}",
    "vm_name": "packer-arista-veos-4.20.1F",
    "disk_image": "true",
    "disk_size": "{{user `disk_size`}}",
    "disk_interface": "ide",
    "headless": "{{user `headless`}}",
    "boot_wait": "120s",
    "boot_command": [
      "admin<enter><wait>",
      "enable<enter><wait>",
      "bash<enter><wait>",
      "sudo dhclient ma1<enter><wait5>",
      "exit<enter><wait>",
      "configure<enter><wait>",
      "username admin privilege 15 role network-admin secret admin<enter>",
      "exit<enter><wait>"
    ],
    "ssh_timeout": "{{user `ssh_timeout`}}",
    "ssh_username": "admin",
    "ssh_password": "admin",
    "qemuargs": [
      ["-m", "{{user `memory`}}"],
      ["-smp", "{{user `cpus`}}"],
      ["-enable-kvm"]
    ]
  }],
  "variables": {
    "compression_level": "6",
    "cpus": "1",
    "disk_size": "40000",
    "headless": "false",
    "iso_checksum": "de89b5ade2f86eeaf7211fa4bae7266eb4410dca",
    "iso_checksum_type": "sha1",
    "memory": "2048",
    "images_path": "file:///{{env `HOME`}}/images",
    "ssh_timeout": "60m"
  }
}
